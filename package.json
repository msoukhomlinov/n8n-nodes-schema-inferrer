{
  "name": "n8n-nodes-schema-inferrer",
  "version": "0.1.0",
  "description": "n8n community node for inferring JSON schemas from sample data",
  "license": "MIT",
  "homepage": "https://github.com/msoukhomlinov/n8n-nodes-schema-inferrer",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "schema",
    "json-schema",
    "typescript"
  ],
  "author": {
    "name": "Max Soukhomlinov",
    "email": "maxs@intellectit.com.au"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/msoukhomlinov/n8n-nodes-schema-inferrer.git"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc && npm run copy:assets && npm run postbuild",
    "build:watch": "tsc --watch",
    "dev": "tsc --watch",
    "copy:assets": "gulp copyAssets",
    "postbuild": "rimraf node_modules/n8n-workflow"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/SchemaInferrer/SchemaInferrer.node.js"
    ]
  },
  "devDependencies": {
    "gulp": "^5.0.1",
    "n8n-workflow": "*",
    "rimraf": "^6.1.0",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
